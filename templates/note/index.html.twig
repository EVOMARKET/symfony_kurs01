{% extends 'base.html.twig' %}

{% block title %}Moje notatki{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 600px; width: 95%; font: 22px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
    <h1>Hello </h1>


   {% for notatka in notatki%}

   <span>Id artykułu: {{notatka.id}}</span></br>
   <span>Tytuł artykułu: {{notatka.Title}}</span></br>
   <span>Treść artykułu: {{notatka.ContentNote}}</span></br>

   {% endfor%}


</div>
{% endblock %}
